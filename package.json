{
  "name": "@bmd-studio/genstack-container-pg-jwt-auth",
  "private": true,
  "main": "dist/index.js",
  "types": "dist/index",
  "scripts": {
    "setup": "yarn install",
    "start": "NODE_OPTIONS=\"--inspect=0.0.0.0:$DEFAULT_DEBUG_PORT\" ts-node src/index.ts",
    "start:dev": "pm2-runtime pm2.config.js",
    "start:prod": "node dist/index.js",
    "test": "yarn test:integration",
    "test:debug": "cross-env DEBUG=pg-jwt-auth:* yarn test",
    "test:integration": "jest --runInBand --testPathPattern=integration/",
    "build": "tsc",
    "build:watch": "yarn build --watch",
    "clean": "rimraf dist/ && rimraf coverage/ && rimraf node_modules/",
    "dc": "docker-compose --env-file=./env/defaults -f docker-compose.yml --compatibility",
    "dc:dev": "yarn dc -f docker-compose.development.yml"
  },
  "dependencies": {
    "apollo-server-express": "2.12.0",
    "bcrypt": "4.0.1",
    "chalk": "4.1.0",
    "debug": "4.1.1",
    "graphql": "14.6.0",
    "jsonwebtoken": "8.5.1",
    "lodash": "4.17.15",
    "pg": "7.18.2",
    "uuid": "8.3.0"
  },
  "devDependencies": {
    "@types/bcrypt": "3.0.0",
    "@types/debug": "4.1.5",
    "@types/jest": "26.0.10",
    "@types/jsonwebtoken": "8.3.9",
    "@types/lodash": "4.14.150",
    "@types/pg": "7.14.3",
    "@types/supertest": "2.0.10",
    "@types/uuid": "7.0.4",
    "cross-env": "7.0.2",
    "get-port": "5.1.1",
    "http-status-codes": "1.4.0",
    "jest": "25.5.0",
    "pm2": "4.4.1",
    "rimraf": "3.0.2",
    "supertest": "4.0.2",
    "testcontainers": "2.20.0",
    "ts-jest": "25.4.0",
    "ts-node": "8.9.1",
    "typescript": "3.8.3"
  }
}
